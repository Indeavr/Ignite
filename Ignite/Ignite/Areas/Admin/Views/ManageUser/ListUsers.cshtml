@model IEnumerable<Ignite.Areas.Admin.ViewModels.ManageUserViewModel>
@{
    ViewBag.Title = "ListUsers";
}

<h2>ListUsers</h2>

@foreach (var user in Model)
{
    <div>
        @user.Username
        @Ajax.ActionLink("Edit", "EditUser",
       new { username = user.Username },
       new AjaxOptions
       {
           InsertionMode = InsertionMode.Replace,
           UpdateTargetId = "editArea"
       })
    </div>
}
<div id="editArea"></div>
@section scripts {
    @Scripts.Render("~/bundles/jqueryajax")
}
